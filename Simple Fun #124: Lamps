function lamps(a) {
  let count0 = 0
  let count1 = 0
  for(let i = 0; i < a.length; i++){
    if(i%2 === 0){
      if(a[i] === 1){
        count0 += 1
      }
      if(a[i] === 0){
        count1 += 1
      }
      }
    if(i%2 === 1){
      if(a[i] === 1){
        count1 += 1
      }
      if(a[i] === 0){
        count0 += 1
      }
    }
  }
  return Math.min(count0, count1)
  //coding and coding..
}
