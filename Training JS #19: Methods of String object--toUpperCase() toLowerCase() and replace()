function alienLanguage(str){
  const arr = str.split(' ')
  const resArr = []
  for(let i = 0; i < arr.length; i++){
    let tmpStr = arr[i].slice(0, arr[i].length - 1).toUpperCase() + arr[i].slice(arr[i].length - 1).toLowerCase()
    resArr.push(tmpStr)
  }
  return resArr.join(' ')
  //coding here...
}
