function swapTwo(array, a, b) {
  let arr = [...array]
  for(let i = 0; i < array.length; i++){
    if(arr[i] === a){
      array[i] = b
      break
    }
  }
  for(let i = array.length - 1; i >= 0; i--){
    if(arr[i] === b){
      array[i] = a
      break
    }
  }
  return [...array]
  // good luck
}
