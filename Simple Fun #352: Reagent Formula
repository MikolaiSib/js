function isValid(formula){
  if(formula.includes(1) && formula.includes(2)){
    return false
  }
  if(formula.includes(3) && formula.includes(4)){
    return false
  }
  if(formula.includes(5)){
    if(formula.includes(6)){
  } else {
    return false
    }
  }
  if(formula.includes(6)){
    if(formula.includes(5)){
  } else {
    return false
    }
  }
  if(!formula.includes(7) && !formula.includes(8)){
    return false
  }
  return true
  //coding and coding..
}
