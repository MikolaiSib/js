function calculateRatio(w, h){
  if(w === 0 || h === 0){
    return 'Error'
  }
  if(w === h ){
    return '1:1'
  }
  if(w /2 === h ){
    return '2:1'
  }
  if(w / 16 === h / 9 ){
    return '16:9'
  }
  if(w / 16 === h / 10 ){
    return '16:10'
  }
  if(w / 4 === h / 3 ){
    return '4:3'
  }
  if(w / 3 === h / 4 ){
    return '3:4'
  }
  if(w / 683 === h / 384 ){
    return '683:384'
  }
  if(w / 183 === h / 50 ){
    return '183:50'
  }
  return w + ':' + h
}
